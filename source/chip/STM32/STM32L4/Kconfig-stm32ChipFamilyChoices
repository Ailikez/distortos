config CHIP_STM32L4
	bool "STM32L4"
	select ARCHITECTURE_ARMV7_M
	select ARCHITECTURE_ARM_CORTEX_M4
	select ARCHITECTURE_ARM_CORTEX_M4_R0P1
	select ARCHITECTURE_HAS_FPU
	help
		STM32L4 chip family