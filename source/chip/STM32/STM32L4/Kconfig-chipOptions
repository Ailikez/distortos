if CHIP_STM32L4

comment "STM32L4 chip options"

menu "Clocks configuration"

config CHIP_STM32L4_RCC_SYSCLK_FREQUENCY
	int "Frequency of SYSCLK, Hz"
	range 1 80000000
	default 4000000
	help
		Frequency of SYSCLK, Hz. RCC must be configured by user to achieve that
		frequency.

choice
	prompt "AHB divider"
	default CHIP_STM32L4_RCC_AHB_DIV1
	help
		AHB clock division factor

		AHBclk = SYSclk / AHBdivider

config CHIP_STM32L4_RCC_AHB_DIV1
	bool "1"
	help
		System clock not divided

config CHIP_STM32L4_RCC_AHB_DIV2
	bool "2"
	help
		System clock divided by 2

config CHIP_STM32L4_RCC_AHB_DIV4
	bool "4"
	help
		System clock divided by 4

config CHIP_STM32L4_RCC_AHB_DIV8
	bool "8"
	help
		System clock divided by 8

config CHIP_STM32L4_RCC_AHB_DIV16
	bool "16"
	help
		System clock divided by 16

config CHIP_STM32L4_RCC_AHB_DIV64
	bool "64"
	help
		System clock divided by 64

config CHIP_STM32L4_RCC_AHB_DIV128
	bool "128"
	help
		System clock divided by 128

config CHIP_STM32L4_RCC_AHB_DIV256
	bool "256"
	help
		System clock divided by 256

config CHIP_STM32L4_RCC_AHB_DIV512
	bool "512"
	help
		System clock divided by 512

endchoice

choice
	prompt "APB1 (low speed) divider"
	default CHIP_STM32L4_RCC_APB1_DIV1
	help
		APB1 (low speed) clock division factor.

		APB1clk = AHBclk / APB1divider

config CHIP_STM32L4_RCC_APB1_DIV1
	bool "1"
	help
		AHB clock not divided

config CHIP_STM32L4_RCC_APB1_DIV2
	bool "2"
	help
		AHB clock divided by 2

config CHIP_STM32L4_RCC_APB1_DIV4
	bool "4"
	help
		AHB clock divided by 4

config CHIP_STM32L4_RCC_APB1_DIV8
	bool "8"
	help
		AHB clock divided by 8

config CHIP_STM32L4_RCC_APB1_DIV16
	bool "16"
	help
		AHB clock divided by 16

endchoice

choice
	prompt "APB2 (high speed) divider"
	default CHIP_STM32L4_RCC_APB2_DIV1
	help
		APB2 (high speed) clock division factor.

		APB2clk = AHBclk / APB2divider

config CHIP_STM32L4_RCC_APB2_DIV1
	bool "1"
	help
		AHB clock not divided

config CHIP_STM32L4_RCC_APB2_DIV2
	bool "2"
	help
		AHB clock divided by 2

config CHIP_STM32L4_RCC_APB2_DIV4
	bool "4"
	help
		AHB clock divided by 4

config CHIP_STM32L4_RCC_APB2_DIV8
	bool "8"
	help
		AHB clock divided by 8

config CHIP_STM32L4_RCC_APB2_DIV16
	bool "16"
	help
		AHB clock divided by 16

endchoice

endmenu

menu "Memory configuration"

config CHIP_STM32L4_FLASH_PREFETCH_ENABLE
	bool "Enable flash prefetch"
	default y
	help
		Enable flash prefetch option in FLASH->ACR register.

config CHIP_STM32L4_FLASH_DATA_CACHE_ENABLE
	bool "Enable flash data cache"
	default y
	help
		Enable flash data cache option in FLASH->ACR register.

config CHIP_STM32L4_FLASH_INSTRUCTION_CACHE_ENABLE
	bool "Enable flash instruction cache"
	default y
	help
		Enable flash instruction cache option in FLASH->ACR register.

choice
	prompt "Unify RAM in linker script"
	depends on CHIP_STM32L43 || CHIP_STM32L44 || CHIP_STM32L45 || CHIP_STM32L46 || CHIP_STM32L49 || CHIP_STM32L4A
	default CHIP_STM32L4_UNIFY_SRAM1_SRAM2
	help
		Configures unification of SRAM1 and SRAM2 regions in linker script.

config CHIP_STM32L4_UNIFY_NONE
	bool "No unification"
	help
		Don't unify any SRAM regions. .bss, .data, stacks and heap will be
		located in SRAM1. Special .sram2.bss, .sram2.data and .sram2.noinit
		sections will be located in SRAM2.

config CHIP_STM32L4_UNIFY_SRAM1_SRAM2
	bool "SRAM1 and SRAM2"
	help
		Unify SRAM1 and SRAM2 regions. .bss, .data, stacks and heap will be
		located in unified SRAM1 and SRAM2.

endchoice

endmenu

