choice
	prompt "STM32 chip family"
	depends on CHIP_STM32
	help
		Select STM32 chip family

# don't remove next line!
### begin Kconfig-stm32ChipFamilyChoices
source "$DISTORTOS_PATH/source/chip/STM32/STM32F1/Kconfig-stm32ChipFamilyChoices"
source "$DISTORTOS_PATH/source/chip/STM32/STM32F4/Kconfig-stm32ChipFamilyChoices"
### end Kconfig-stm32ChipFamilyChoices
# don't remove previous line!

endchoice
