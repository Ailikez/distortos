config CHIP_STM32F4_SRAM2_SIZE
	int
	default 16384 if CHIP_STM32F405 || CHIP_STM32F407 || CHIP_STM32F415 || CHIP_STM32F417 || CHIP_STM32F42 || \
		CHIP_STM32F43 || CHIP_STM32F44
	default 32768 if CHIP_STM32F46 || CHIP_STM32F47
	default 0

config CHIP_STM32F4_SRAM2_ADDRESS
	hex
	default 0x20028000 if CHIP_STM32F46 || CHIP_STM32F47
	default 0x2001C000
	depends on CHIP_STM32F4_SRAM2_SIZE != 0

config CHIP_STM32F4_SRAM3_SIZE
	int
	default 65536 if CHIP_STM32F42 || CHIP_STM32F43
	default 131072 if CHIP_STM32F46 || CHIP_STM32F47
	default 0

config CHIP_STM32F4_SRAM3_ADDRESS
	hex
	default 0x20030000 if CHIP_STM32F46 || CHIP_STM32F47
	default 0x20020000
	depends on CHIP_STM32F4_SRAM3_SIZE != 0

